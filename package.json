{
  "name": "mean-start",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "postinstall": "npm run build",
    "build": "ng build -prod && tsc --project server",
    "start": "pm2 start dist/www.js --no-daemon",
    "build-start": "npm run build && npm start",
    "dev": "concurrently \"ng serve --progress false --proxy-config proxy.conf.json\" \"npm run api\" ",
    "api": "cd server && nodemon --ext ts --ignore *.spec.ts --exec ts-node www",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "concurrently \"ng test --progress false\" \"npm run test-api\" ",
    "test-ng": "ng test",
    "test-api": "cd server && nodemon --ext ts --exec jasmine-ts **/*.spec.ts",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.18",
    "@types/body-parser": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.34",
    "@types/jasmine": "2.5.38",
    "@types/mongoose": "^4.7.1",
    "@types/morgan": "^1.7.32",
    "@types/nock": "^8.2.1",
    "@types/node": "^6.0.42",
    "@types/request": "0.0.39",
    "@types/socket.io": "^1.4.27",
    "@types/socket.io-client": "^1.4.29",
    "angular-cli": "1.0.0-beta.26",
    "axios": "^0.15.3",
    "body-parser": "^1.15.2",
    "bootstrap": "4.0.0-alpha.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.4",
    "debug": "^2.3.3",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "mongoose": "^4.7.2",
    "morgan": "^1.7.0",
    "pm2": "^2.4.0",
    "request": "^2.79.0",
    "require-all": "^2.1.0",
    "rxjs": "^5.0.1",
    "socket.io": "^1.7.2",
    "ts-helpers": "^1.1.1",
    "typescript": "~2.0.3",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.34",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "jasmine-ts": "0.0.3",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "nock": "^9.0.9",
    "node-mocks-http": "^1.6.1",
    "nodemon": "^1.11.0",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0"
  },
  "engines": {
    "node": "6.9.2"
  }
}
